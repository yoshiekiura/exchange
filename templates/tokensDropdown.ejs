<a href="#" class="dropdown-toggle" data-toggle="dropdown" role="button" aria-haspopup="true" aria-expanded="false" style="font-size: 135%; color: #036;"><%= selectedToken.name %> <span class="caret"></span></a>
<ul class="dropdown-menu">
  <% 
    tokens.forEach(function(token, i, array) {
    if(token.addr == '0x0000000000000000000000000000000000000000') return; // skip eth
  %>
    <li><a href="javascript:;" onclick="bundle.Cryptex.selectToken('<%= token.addr %>');"><%= token.name %></a></li>
  <% }); %>
  <li role="separator" class="divider"></li>
  <li><a href="javascript:;" data-toggle="modal" data-target="#otherTokenModal"><span class="trn">other</span></a></li>
</ul>
